{
	"$schema": "https://typedoc.org/schema.json",
	"alwaysCreateEntryPointModule": false,
	"groupReferencesByType": false,
	"cleanOutputDir": true,
	"customCss": "./styles.css",
	"entryPointStrategy": "packages",
	"entryPoints": [
		"../libs/*"
	],
	"packageOptions": {
		"entryPoints": [
			"src/index.ts"
		]
	},
	"excludeInternal": true,
	"excludePrivate": true,
	"hideGenerator": true,
	"jsDocCompatibility": {
		"exampleTag": false
	},
	"navigation": {
		"includeGroups": true
	},
	"out": "./dist",
	"readme": "../README.md",
	"searchInComments": true
}
