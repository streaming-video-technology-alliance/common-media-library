{
	"$schema": "https://typedoc.org/schema.json",
	"categorizeByGroup": true,
	"cleanOutputDir": true,
	"entryPoints": [
		"../lib/src/index.ts"
	],
	"excludeInternal": true,
	"excludePrivate": true,
	"hideGenerator": true,
	"jsDocCompatibility": {
		"exampleTag": false
	},
	"navigation": {
		"includeGroups": true
	},
	"out": "./dist",
	"readme": "../lib/README.md",
	"searchInComments": true
}
